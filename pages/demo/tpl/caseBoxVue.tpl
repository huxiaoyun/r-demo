<div class="case-box">
  <div class="index-bg">
  </div>
  <div class="index">
    {{#add i 1}}{{/add}}
  </div>
  <div class="case-demo">
    {{{tpl}}}
  </div>
  <div class="case-split"></div>
  <div class="case-code">
    <pre class="case-code-detail" id="code{{i}}"></pre>
    <div class="op">
      <a class="run" data-index="{{i}}">
        <i class="iconfont icon-yunxing"></i>
        试一试
      </a>
    </div>
  </div>
</div>
